# Uncomment the next line to define a global platform for your project
platform :ios, '11.0'
source 'https://github.com/CocoaPods/Specs.git'
inhibit_all_warnings!

target 'iOSVendors' do
  # Uncomment the next line if you're using Swift or would like to use dynamic frameworks
  # use_frameworks!
  
  # 网络
  pod 'AFNetworking'
  # 空白页
  pod 'DZNEmptyDataSet'
  # 自动布局
  pod 'Masonry'
  # HUD 提示框
  pod 'MBProgressHUD'
  # 自定义弹出框
  pod 'MMPopupView-Enhanced'
  # 下拉上拉刷新
  pod 'MJRefresh'
  # 键盘管理
  pod 'IQKeyboardManager'
  # 轮播图
#  pod 'SDCycleScrollView'
  # 网络图片加载缓存
#  pod 'SDWebImage'

  # 模型转换
  pod 'YYModel'
  # 富文本
  pod 'YYText'
  # 图片选择器
  pod 'TZImagePickerController'
  # cover flow
  pod 'iCarousel'
  # 数据库
  pod 'FMDB'
  # 分页选择
  pod 'SGPagingView'

  target 'iOSVendorsTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'iOSVendorsUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
